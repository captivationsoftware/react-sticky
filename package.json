{
  "name": "react-sticky",
  "version": "6.0.1",
  "description": "Sticky component for React",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib es",
    "build:commonjs": "babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "start": "webpack --watch",
    "prepublish": "npm run clean && npm run build",
    "test": "cross-env BABEL_ENV=development NODE_ENV=test mocha test/setup.js test/spec/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/captivationsoftware/react-sticky"
  },
  "keywords": [
    "react-component",
    "React",
    "Sticky"
  ],
  "author": "Captivation Software",
  "license": "MIT",
  "homepage": "https://github.com/captivationsoftware/react-sticky",
  "dependencies": {
    "prop-types": "^15.5.8",
    "raf": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^15.3.0",
    "react-dom": "^15.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cross-env": "^5.0.5",
    "enzyme": "^2.8.2",
    "jsdom": "8.0.4",
    "mocha": "^2.4.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.2",
    "webpack": "^2.2.0"
  }
}
